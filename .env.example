# MiniMax API Configuration
VITE_MINIMAX_API_KEY=your_minimax_api_key_here
VITE_MINIMAX_BASE_URL=https://api.minimax.chat/v1

# Default Model Configuration
VITE_DEFAULT_MODEL=minimax-m2
VITE_MAX_TOKENS=2048
VITE_TEMPERATURE=0.7

# Application Configuration
VITE_APP_TITLE=Chat-UI
VITE_APP_DESCRIPTION=Conversational AI Interface

# Features
VITE_ENABLE_STREAMING=true
VITE_ENABLE_TOOLS=true
VITE_ENABLE_HISTORY=true
VITE_ENABLE_EXPORT=true

# Development
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info